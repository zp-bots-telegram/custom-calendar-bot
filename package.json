{
  "name": "custom-calendar-bot",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "https:git@github.com:zp-bots-telegram/custom-calendar-bot"
  },
  "description": "Bot to take in files and conver them to ical links",
  "main": "src/handler.ts",
  "scripts": {
    "postinstall": "is-ci || husky install",
    "build": "webpack --progress --color",
    "lint": "eslint ./src --ext .ts",
    "lint:fix": "eslint ./src --fix --ext .ts",
    "prettier:format": "prettier --config .prettierrc \"src/**/*.ts\" --write",
    "start": "node dist/bundle.js",
    "start:dev": "nodemon -r dotenv/config --config nodemon.json",
    "test": "yarn test:unit",
    "test:unit": "jest src",
    "test:unit:coverage": "jest src --collectCoverage",
    "lint-staged": "lint-staged"
  },
  "keywords": [],
  "author": "Zack Pollard (https://github.com/zp-bots-telegram)",
  "license": "ISC",
  "dependencies": {
    "@types/node-fetch": "^2.6.1",
    "cheerio": "^1.0.0-rc.10",
    "express": "^4.17.3",
    "grammy": "^1.7.0",
    "ical-generator": "^3.2.1",
    "telegraf": "^4.7.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^16.2.1",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^8.0.2",
    "@types/debug": "^4.1.7",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.21",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.13.0",
    "@typescript-eslint/parser": "^5.13.0",
    "commitlint": "^16.2.1",
    "dotenv": "^16.0.0",
    "eslint": "^8.10.0",
    "eslint-config-prettier": "^8.4.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest": "^26.1.1",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "is-ci": "^3.0.1",
    "jest": "^27.5.1",
    "lint-staged": "^12.3.4",
    "localtunnel": "^2.0.2",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "semantic-release": "^19.0.2",
    "ts-jest": "^27.1.3",
    "ts-loader": "^9.2.7",
